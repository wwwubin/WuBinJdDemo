<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="@drawable/reg_bg"
        android:id="@+id/reg"
        android:layout_marginTop="-5.5dp"/>
    <ImageView
        android:layout_marginTop="20dp"
        android:layout_marginLeft="290dp"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/my_set_bai"/>
    <ImageView
        android:layout_marginTop="20dp"
        android:layout_marginLeft="330dp"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/my_msg_bai"/>
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/user"
        android:layout_margin="50dp"
        />
    <TextView
        android:id="@+id/dlzc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="登录/注册>"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="100dp"/>
    <LinearLayout
        android:layout_marginTop="150dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="20dp"
                android:gravity="center">
                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/dfk" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="代付款"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/wd_left"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/dsh" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="代收货"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/wd_left"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/dpj" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="代评价"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/wd_left"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/th_sh" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="退换/售后"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="30dp"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/mydd" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的订单"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
        </LinearLayout>
        <include layout="@layout/layout_divider"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="20dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="1000"
                    android:textSize="@dimen/wd_textSize" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:text="京豆"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="5张"
                    android:textSize="@dimen/wd_textSize" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:text="优惠券"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:textSize="@dimen/wd_textSize" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:text="白条"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="三张"
                    android:textSize="@dimen/wd_textSize" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:text="京东E卡"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="-10dp"
                    android:src="@drawable/myqb" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:text="我的钱包"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
        </LinearLayout>
        <include layout="@layout/divider_horizontal3"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/spgz" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="商品关注"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="店铺关注"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/gz" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="内容关注"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/lljl" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="浏览记录"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
        </LinearLayout>
        <include layout="@layout/layout_divider"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="30dp"
                android:gravity="center">
                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/hd" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的活动"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="50dp"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/shqz" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="社区"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="50dp"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/wd_height"
                    android:layout_height="@dimen/wd_height"
                    android:src="@drawable/kh" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="客户服务"
                    android:textSize="@dimen/wd_textSize" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
